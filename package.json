{
  "name": "entre-com-unicamp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf next.config.mjs",
    "predev": "yarn build:config",
    "dev": "LOG_LEVEL=http ts-node --project tsconfig.server.json server/index.ts",
    "dev:old": "next dev",
    "prebuild": "yarn build:config && yarn build:server",
    "build": "next build",
    "build:config": "yarn clean && esbuild next.config.ts --bundle --outfile=next.config.mjs --platform=node --format=esm",
    "build:server": "tsc --project tsconfig.server.json",
    "serve": "NODE_ENV=production node dist/server/index.js",
    "lint:types": "tsc --noEmit",
    "lint:eslint": "eslint",
    "lint": "yarn lint:types && yarn lint:eslint .",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "yarn lint:types && yarn lint:eslint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint:staged"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.56.0",
    "@aws-sdk/s3-request-presigner": "^3.56.0",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/api-metrics": "^0.27.0",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/instrumentation-winston": "^0.27.2",
    "@opentelemetry/propagator-jaeger": "^1.0.1",
    "@opentelemetry/sdk-metrics-base": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "@popperjs/core": "^2.11.2",
    "@prisma/client": "3.10.0",
    "classnames": "^2.3.1",
    "cookie": "^0.4.2",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "jose": "^4.6.0",
    "next": "12.1.0",
    "pg-connection-string": "^2.5.0",
    "prisma": "^3.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-popper": "^2.2.5",
    "react-qr-code": "^2.0.3",
    "react-use": "^17.3.2",
    "redis": "^4.0.4",
    "swr": "^1.2.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@marolint/eslint-config-nextjs": "^1.0.2",
    "@types/cookie": "^0.4.1",
    "@types/express": "^4.17.13",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.25",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "4.6.2",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.70.0"
  }
}
