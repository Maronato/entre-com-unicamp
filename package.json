{
  "name": "entre-com-unicamp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf next.config.mjs",
    "predev": "yarn build:config",
    "dev": "next dev",
    "prebuild": "yarn build:config",
    "build": "next build",
    "build:config": "yarn clean && esbuild next.config.ts --bundle --outfile=next.config.mjs --platform=node --format=esm",
    "serve": "next start",
    "lint:types": "tsc --noEmit",
    "lint:eslint": "eslint",
    "lint": "yarn lint:types && yarn lint:eslint .",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "yarn lint:types && yarn lint:eslint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint:staged"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@popperjs/core": "^2.11.2",
    "@prisma/client": "3.10.0",
    "@supabase/supabase-js": "^1.31.1",
    "classnames": "^2.3.1",
    "cookie": "^0.4.2",
    "date-fns": "^2.28.0",
    "jose": "^4.6.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-popper": "^2.2.5",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@marolint/eslint-config-nextjs": "^1.0.1",
    "@types/cookie": "^0.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "esbuild": "^0.14.25",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "prisma": "^3.10.0",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
